{
  "name": "@segment/analytics-react-native-example",
  "description": "Example app for @segment/analytics-react-native",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "e2e:build:android": "yarn detox build --configuration android",
    "e2e:test:android": "yarn detox test --configuration android",
    "e2e:build:ios": "yarn detox build --configuration ios",
    "e2e:test:ios": "yarn detox test --configuration ios",
    "start": "react-native start",
    "start:e2e": "RN_SRC_EXT=e2e.mock.ts,e2e.mock.js yarn start",
    "android:deeplink": "adb shell am start -a android.intent.action.VIEW -d \"segmentreactnative://hello\" com.example.segmentanalyticsreactnative",
    "ios:deeplink": "xcrun simctl openurl booted segmentreactnative://hello",
    "android:emulator": "./android/device_setup.sh"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^1.15.7",
    "@react-native-community/masked-view": "^0.1.11",
    "@react-navigation/native": "^6.0.2",
    "@react-navigation/stack": "^6.0.7",
    "react": "17.0.2",
    "react-native": "0.65.1",
    "react-native-bootsplash": "^3.2.4",
    "react-native-gesture-handler": "^1.10.3",
    "react-native-safe-area-context": "^3.3.0",
    "react-native-tracking-transparency": "^0.1.1",
    "react-native-fbsdk-next": "^4.6.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/runtime": "^7.12.5",
    "@react-native-community/eslint-config": "^2.0.0",
    "babel-jest": "^26.6.3",
    "babel-plugin-module-resolver": "^4.1.0",
    "body-parser": "^1.19.0",
    "detox": "^18.23.1",
    "eslint": "7.14.0",
    "express": "^4.17.1",
    "jest": "^26.6.3",
    "jest-circus": "^26.6.3",
    "metro-react-native-babel-preset": "^0.66.0",
    "react-native-codegen": "^0.0.7",
    "react-test-renderer": "17.0.2"
  }
}
